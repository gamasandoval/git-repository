{
  "client_id": "PRCC",
  "client_name": "Pearl River Community College",
  "os_family": "rhel8",
  "defaults": {
    "use_sudo": true,
    "init_system": "systemd"
  },
  "hosts": {
    "it-s-banethos-t": {
      "environment": "TEST",
      "description": "PRCC TEST - BanETHOS Tomcat host",
      "init_system": "systemd",
      "apps": {
        "BEP": {
          "display_name": "Banner Event Publisher",
          "tomcat": {
            "run_as_user": "tomcat",
            "service_unit": "tc_TEST8080_bep.service",
            "base_dir": "/u01/app/tomcat/tc_TEST8080_bep",
            "port": 8080,
            "logs": {
              "catalina": "/u01/app/tomcat/tc_TEST8080_bep/logs/catalina.out",
              "access": "/u01/app/tomcat/tc_TEST8080_bep/logs/localhost_access_log*.txt",
              "event_summary": "/u01/app/tomcat/tc_TEST8080_bep/logs/bep_event_summary.log",
              "event_publisher": "/u01/app/tomcat/tc_TEST8080_bep/logs/BannerEventPublisher.log"
            },
            "url": "http://it-s-banethos-t.prcc.local:8080/BannerEventPublisher/login/auth"
          }
        },
        "EMA": {
          "display_name": "Ellucian Messaging Adaptor",
          "tomcat": {
            "run_as_user": "tomcat",
            "service_unit": "tc_TEST8081_ema.service",
            "base_dir": "/u01/app/tomcat/tc_TEST8081_ema",
            "port": 8081,
            "logs": {
              "catalina": "/u01/app/tomcat/tc_TEST8081_ema/logs/catalina.out",
              "access": "/u01/app/tomcat/tc_TEST8081_ema/logs/localhost_access_log*.txt",
              "ema_log": "/u01/app/tomcat/tc_TEST8081_ema/logs/ema.log"
            },
            "url": null
          }
        }
      }
    },
    "it-s-dw-web-t": {
      "environment": "TEST",
      "description": "PRCC TEST - DegreeWorks host",
      "init_system": "systemd",
      "apps": {
        "DW": {
          "display_name": "Degree Works",
          "service": {
            "run_as_user": "dwadmin",
            "service_unit": "degreeworks.target",
            "base_dir": "/u01/app/ellucian/dw/dwtest",
            "port": null,
            "logs": {
              "dashboard": "/u01/app/ellucian/dw/dwtest/scripts/logs/dashboard.log",
              "controller": "/u01/app/ellucian/dw/dwtest/scripts/logs/controller.log",
              "transit": "/u01/app/ellucian/dw/dwtest/scripts/logs/transitAPI.log",
              "http_error": "/usr/local/apache/logs/error_log",
              "http_access": "/usr/local/apache/logs/access_log"
            },
            "url": "https://dw-t.prcc.edu/ResponsiveDashboard/login.html"
          },
          "services": [
            "degreeworks.target",
            "httpd.service",
            "degreeworks-transit.service",
            "degreeworks-controller.service",
            "degreeworks-responsiveDashboard.service"
          ],
          "components": {
            "DW_HTTP": {
              "display_name": "Degree Works HTTPD Service",
              "run_as_user": "dwadmin",
              "service_unit": "httpd.service",
              "base_dir": "/usr/local/apache/",
              "port": 443,
              "logs": {
                "error": "/usr/local/apache/logs/error_log",
                "access": "/usr/local/apache/logs/access_log"
              },
              "url": null
            },
            "DW_TRANSIT": {
              "display_name": "Degree Works Transit Service",
              "run_as_user": "dwadmin",
              "service_unit": "degreeworks-transit.service",
              "base_dir": "/u01/app/ellucian/dw/dwtest",
              "port": 8448,
              "logs": {
                "transit": "/u01/app/ellucian/dw/dwtest/scripts/logs/transitAPI.log"
              },
              "url": "https://dw-t.prcc.edu/Transit/login.html"
            },
            "DW_CONTROLLER": {
              "display_name": "Degree Works Controller Service",
              "run_as_user": "dwadmin",
              "service_unit": "degreeworks-controller.service",
              "base_dir": "/u01/app/ellucian/dw/dwtest",
              "port": 8446,
              "logs": {
                "controller": "/u01/app/ellucian/dw/dwtest/scripts/logs/controller.log"
              },
              "url": "https://dw-t.prcc.edu/Controller/login.html"
            },
            "DW_DASHBOARD": {
              "display_name": "Degree Works Dashboard Service",
              "run_as_user": "dwadmin",
              "service_unit": "degreeworks-responsiveDashboard.service",
              "base_dir": "/u01/app/ellucian/dw/dwtest",
              "port": 8447,
              "logs": {
                "dashboard": "/u01/app/ellucian/dw/dwtest/scripts/logs/dashboard.log"
              },
              "url": "https://dw-t.prcc.edu/ResponsiveDashboard/login.html"
            }
          }
        }
      }
    }
  }
}
